{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Permission Schema",
  "type": "object",
  "properties": {
    "user_id": {
      "type": "string"
    },
    "permissions": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  },
  "required": ["user_id", "permissions"]
}